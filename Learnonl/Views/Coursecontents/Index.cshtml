@model IEnumerable<Learnonl.Data.Coursecontent>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Subjecttitle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LessoncontentId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Lesson)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Subjecttitle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LessoncontentId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lesson.LessonId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CoursecontentId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CoursecontentId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CoursecontentId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
